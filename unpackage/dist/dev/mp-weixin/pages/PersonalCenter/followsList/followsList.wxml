<view><block wx:if="{{!followsList.length}}"><no-data vue-id="de27e754-1" text="快去关注商家吧~" url="https://xssj.letterbook.cn/applet/images/home_empty_bg.png" bind:__l="__l"></no-data></block><view class="follow-list"><block wx:for="{{followsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><van-swipe-cell class="follow-list-cell van-swipe-cell" vue-id="{{'de27e754-2-'+index}}" right-width="{{' '+65+' '}}" bind:__l="__l" vue-slots="{{['default','right']}}"><navigator url="{{'/pages/shopHome/home/home?marchantId='+item.id}}"><view class="follow-item"><view style="display:flex;"><image class="touxiang" src="{{item.logoPic}}" mode="aspectFill"></image><view class="follow-content"><view style="display:flex;margin:14rpx 0 12rpx;align-items:center;"><view class="follow-name">{{item.nickName}}</view></view><view class="follow-content-info"><block wx:for="{{5}}" wx:for-item="item" wx:for-index="index1" wx:key="index1"><view class="follow-star-list"><image style="width:20rpx;height:20rpx;margin-right:2rpx;" src="/static/pages/img/my/star.png"></image></view></block><text style="font-size:22rpx;font-weight:400;line-height:30rpx;color:#da502d;height:30rpx;margin-left:4rpx;">5分</text><view style="width:1px;height:19rpx;background:#d2d2d2;margin-left:8rpx;margin-right:8rpx;"></view><view class="looked-man">{{(item.accessNumber||0)+"访问数"}}</view></view><view class="label-list"><block wx:for="{{item.labelsList}}" wx:for-item="item" wx:for-index="index1" wx:key="index1"><view class="label-item">{{item}}</view></block></view></view></view><view class="follow-content-botton">{{"简介:"+item.brief}}</view></view></navigator><view class="del" slot="right"><view class="delDiv" data-id="{{item.id}}" data-index="{{index}}" data-event-opts="{{[['tap',[['cancelFollow',['$event']]]]]}}" bindtap="__e"><text>取消\n关注</text></view></view></van-swipe-cell></block></view></view>
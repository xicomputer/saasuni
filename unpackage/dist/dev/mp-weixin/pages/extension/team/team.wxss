page {
    min-height: 100%;
    width: 100%;
    height: 100%;
    overflow-x: hidden;
    background-color: #f7f7f7 !important;
}
.u-line-1 {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.u-line-2 {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}


page {
    min-height: 100%;
    width: 100%;
    height: 100%;
    overflow-x: hidden;
    background-color: #f7f7f7 !important;
}
.u-line-1 {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.u-line-2 {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.member_tool {
    height: 122rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    /* border-bottom: 2rpx solid #F4F4F4; */
}
.member_tool_item_line {
    width: 0rpx;
    height: 78rpx;
    border: 1rpx solid #f4f4f4;
}
.member_tool_item {
    flex: 1;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.member_tool_item_name {
    font-size: 28rpx;
    font-weight: 400;
    line-height: 34rpx;
    color: #ffffff;
}
.member_tool_item_desc {
    font-size: 32rpx;
    font-weight: bold;
    line-height: 40rpx;
    color: #ffffff;
}


    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.nocity_img {
    width: 570rpx;
    height: 570rpx;
}
.nocity_close_img {
    margin-top: 70rpx;
    width: 48rpx;
    height: 48rpx;
}
.sale_money {
    padding-left: 20rpx;

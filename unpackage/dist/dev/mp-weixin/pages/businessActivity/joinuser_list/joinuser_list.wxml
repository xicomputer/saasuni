<view><block wx:if="{{userList.length}}"><view class="join-user-list"><block wx:for="{{userList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view style="margin-bottom:40rpx;"><block wx:if="{{item.financeUnitResponses.length}}"><view class="earnings-msg">{{'已推'+(item.alreadyOrder||0)+"单，收益"+(item.income||0)+"元，还可以获得"+(item.residueIncome||0)+'元收益'}}</view></block><block wx:for="{{item.financeUnitResponses}}" wx:for-item="temp" wx:for-index="ti" wx:key="ti"><view class="me-fx-row me-fx-sb-c pushed-item"><view class="me-fx-row me-fx-c-c"><image class="user-avatar-img" src="{{temp.headimgurl}}"></image><text>{{temp.nickname}}</text></view><view class="me-fx-row me-fx-c-c"><text class="amount-str">{{"+"+temp.earnings+"元"}}</text><block wx:if="{{temp.isLast==true}}"><text class="service-fee">{{"(服务费¥"+temp.serviceCharge+")"}}</text></block></view></view></block></view></block></view></block><block wx:else><view class="me-fx-col me-fx-c-c list-empty-box"><text>暂无数据</text></view></block></view>
<view><view class="activity-list-container me-fx-col"><block wx:for="{{dataList}}" wx:for-item="temp" wx:for-index="ti" wx:key="ti"><block><block wx:if="{{temp.activityList.list.length}}"><view class="class-item-top-box"><view class="me-fx-row me-fx-c-c"><text class="class-type-name">{{temp.classifyName}}</text><text class="view-more" data-classid="{{temp.id}}" data-classname="{{temp.classifyName}}" data-event-opts="{{[['tap',[['toMoreList',['$event']]]]]}}" bindtap="__e">查看更多></text></view></view></block><block wx:for="{{temp.activityList.list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['activity-list-info me-fx-col activity-'+item.activityId]}}" data-atyid="{{item.activityId}}" data-event-opts="{{[['tap',[['toDetail',['$event']]]]]}}" bindtap="__e"><view class="activity-top"><image class="activity-img" mode="aspectFill" src="{{item.imageUrl}}"></image><block wx:if="{{item.activityJoinNumberTotal}}"><view class="me-fx-row me-fx-sb-c activity-num down-order-num"><text class="triangle-box"></text><text class="btn_last">{{"已有"+item.activityJoinNumberTotal+"人下单"}}</text></view></block><block wx:if="{{item.residueNumber!=-1}}"><view class="me-fx-row me-fx-sb-c activity-num"><text class="triangle-box"></text><text class="btn_last">{{'剩余名额：'+(item.residueNumber||0)}}</text></view></block></view><view class="activity-foot"><view style="padding:10rpx;box-sizing:border-box;"><view class="me-fx-row me-fx-sb-c foot-line-item"><text class="me-full-text activity-shop-name">{{"活动名称："+item.templateName}}</text><van-count-down vue-id="{{'6935569e-1-'+ti+'-'+index}}" time="{{item.diffTimes}}" auto-start="{{true}}" use-slot="{{true}}" data-tindex="{{ti}}" data-index="{{index}}" data-event-opts="{{[['^change',[['timeChange',['$event',['o',['tindex',ti],['index',index]]]]]],['^finish',[['finishFun',['$event',['o',['tindex',ti],['index',index]]]]]]]}}" bind:change="__e" bind:finish="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="me-fx-row me-fx-start-c times-text"><text>{{(item.state==1?'距开始':'距结束')+"："}}</text><block wx:if="{{item.times.days}}"><view class="time-num">{{item.times.days||0}}</view></block><block wx:if="{{item.times.days}}"><text>天</text></block><view class="time-num">{{item.times.hours}}</view>时<view class="time-num">{{item.times.minutes}}</view>分<view class="time-num">{{item.times.seconds}}</view>秒</view></van-count-down></view><view class="me-fx-row me-fx-sb-c foot-line-item"><text class="me-full-text activity-goods-name">{{item.commodityName}}</text><view class="me-fx-row me-fx-c-end price-box"><text>活动价:¥</text><text class="price-num">{{item.lowPrice}}</text></view></view></view><block wx:for="{{item.meJoinInfo}}" wx:for-item="temp" wx:for-index="ti" wx:key="ti"><view><view class="{{['me-fx-row me-fx-sb-c msg-line-box '+(temp.status==2||temp.status==3?'complete':'not-complete')]}}"><block wx:if="{{temp.status==2||temp.status==3}}"><text>{{'您已完成'+temp.alreadyOrder+"单任务，您购买商品已退还金额("+item.meJoinInfo.income+'元)'}}</text></block><block wx:else><text>{{"已推"+temp.alreadyOrder+"单，商品金额退还"+temp.income+" 只差"+temp.residueOrder+"单赚取商品"}}</text></block><text class="view-detail" data-item="{{temp}}" data-event-opts="{{[['tap',[['jumpJoinDetail',['$event']]]]]}}" catchtap="__e">查看详情</text></view></view></block></view></view></block></block></block></block><block wx:if="{{!dataList.length}}"><view class="me-fx-row me-fx-c-c place-empty"><text>暂无数据</text></view></block></view></view>
<view class="page-content"><van-tabs vue-id="43b8a4de-1" active="{{currentIndex}}" sticky="{{true}}" color="#EC1919" line-width="22" data-event-opts="{{[['^change',[['onChange']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view src="{{item}}"><van-tab vue-id="{{('43b8a4de-2-'+index)+','+('43b8a4de-1')}}" title="{{item.name}}" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{currentList}}" wx:for-item="item" wx:for-index="index1" wx:key="index1"><block><view class="{{['order-item order-'+item.orderNumber]}}"><view class="me-fx-row product-info"><image class="product-img" src="{{item.detailImgUrls[0]}}"></image><view class="me-fx-1 me-fx-col me-fx-sb"><view>{{item.commodityName}}</view><view class="me-fx-row me-fx-sb-c"><text>{{"¥"+item.livePrice}}</text><text>{{"x"+item.amount}}</text></view><view class="price-text">{{"合计:¥"+item.actualMoney}}</view></view></view><view class="receive-goods"><view>{{"订单号:"+item.orderNumber}}</view><view>{{"订单生成时间:"+item.orderTime}}</view><block wx:if="{{item.shippingAddress.contacts}}"><view>{{"收货人: "+(item.shippingAddress.contacts+' '+item.shippingAddress.contactWay)}}</view></block><block wx:if="{{item.shippingAddress.provincesName}}"><view>{{'收货地址:\n                                '+(item.shippingAddress.provincesName+item.shippingAddress.cityName+item.shippingAddress.areaName+item.shippingAddress.address)+''}}</view></block><block wx:if="{{currentState==2}}"><view>{{item.expressCompany+"："+item.expressNo}}</view></block><block wx:if="{{currentState==2}}"><view>{{"商家发货时间："+item.deliveryTime}}</view></block></view></view><view class="order-item-foolt"><block wx:if="{{item.activityFinanceResponse.status==2||item.activityFinanceResponse.status==3}}"><view class="msg-text">{{'已完成：'+item.activityFinanceResponse.alreadyOrder+"单任务，您购买商品已退还金额("+item.activityFinanceResponse.income+'元)'}}<block wx:if="{{item.activityFinanceResponse.status==2}}"><text>{{"服务费："+item.activityFinanceResponse.serviceCharge}}</text></block></view></block><block wx:else><view class="msg-text not-complete">{{'进行中：已推'+item.activityFinanceResponse.alreadyOrder+"单，商品金额退还"+item.activityFinanceResponse.income+" 只差"+item.activityFinanceResponse.residueOrder+'单赚取商品'}}</view></block><block wx:if="item.activityFinanceResponse"><view class="join-user-list"><block wx:for="{{item.activityFinanceResponse.financeUnitResponses}}" wx:for-item="temp" wx:for-index="ti" wx:key="ti"><view class="me-fx-row me-fx-sb-c join-user-item"><view class="me-fx-row me-fx-start-c user-info"><image class="user-avatar" src="{{temp.headimgurl}}"></image><view class="me-full-text nick-name">{{temp.nickname}}</view><view class="create-time">{{"下单时间"+temp.createTime}}</view></view><view class="me-fx-1 earnings-text">{{"+"+temp.earnings+"元"}}</view><view class="me-fx-1 service-charge">{{temp.isLast?'服务费'+(temp.serviceCharge+temp.payPlatformFeeMoney):''}}</view></view></block></view></block><view class="me-fx-row me-fx-end-c foot-btns"><text class="confirm-btn contact-btn" data-item="{{item}}" data-event-opts="{{[['tap',[['toChat',['$event']]]]]}}" bindtap="__e">联系商家</text><block wx:if="{{currentState==2}}"><text class="confirm-btn" data-item="{{item}}" data-event-opts="{{[['tap',[['confirmGoods',['$event']]]]]}}" bindtap="__e">确认收货</text></block></view></view></block></block><block wx:if="{{!currentList.length}}"><view class="me-fx-row me-fx-c-c place-empty"><text>暂无数据</text></view></block></van-tab></view></block></van-tabs></view>
<block wx:if="{{ArticleList.length}}"><view class="culture-box"><view class="me-fx-row me-fx-c-c title-box"><view data-event-opts="{{[['tap',[['goMore',['$event']]]]]}}" class="more" style="{{('color:'+(tempId==2?'#fff':'#333'))}}" bindtap="__e"><text>商家文化</text><text class="icon icon-right"></text></view></view><block wx:if="{{tempId<5}}"><view class="me-fx-row me-fx-sb culture-img-list other_list"><block wx:for="{{ArticleList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="ArticleList" data-link="{{item.linkurl}}" data-event-opts="{{[['tap',[['toWebViewPage',['$event']]]]]}}" bindtap="__e"><image class="culture-img" mode="aspectFit" src="{{item.articlecover}}"></image><view class="ArtiBox me-fx-row" style="{{('color:'+(tempId==2?'#fff':'#333'))}}"><text class="me-full-text">{{item.articletitle}}</text></view></view></block></block></view></block><block wx:if="{{tempId==6}}"><view class="me-fx-col culture-list2"><block wx:for="{{ArticleList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><block wx:if="{{index<3}}"><view class="ArticleList2 me-fx-row me-fx-sb" data-link="{{item.linkurl}}" data-event-opts="{{[['tap',[['toWebViewPage',['$event']]]]]}}" bindtap="__e"><view class="culture2_info me-fx-col me-fx-sb"><text class="culture2_title">{{item.articletitle}}</text><text class="culture2_time">{{item.createtime}}</text></view><image class="culture2-img" mode="aspectFit" src="{{item.articlecover}}"></image></view></block><block wx:if="{{ArticleList.length>3?index<2:ArticleList.length-1!=index}}"><view class="line"></view></block></block></block></view></block></view></block>
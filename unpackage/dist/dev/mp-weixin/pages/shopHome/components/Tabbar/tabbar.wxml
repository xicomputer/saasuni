<view class="custom-tab-bar"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{item.isHave}}"><view class="custom-tab-bar-item" data-text="{{item.text}}" data-url="/pages/shopHome/dynamic/dynamic" data-event-opts="{{[['tap',[['changeTab',['$event']]]]]}}" bindtap="__e"><image class="{{[item.text=='商家文化'?'shopsSty':'custom-Img']}}" src="{{nowTabbarTextClone==item.text?item.imgActive:item.img}}" mode="aspectFill"></image><block wx:if="{{item.text!=='商家文化'}}"><text class="{{[nowTabbarTextClone==item.text?'custom-tab-bar-text custom-tab-bar-text-active':'custom-tab-bar-text']}}">{{''+item.text+''}}</text></block></view></block></block></view>
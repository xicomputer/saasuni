<view class="list"><image class="list_myPm" src="https://xssj.letterbook.cn/applet/images/icon_myPm.png" data-event-opts="{{[['tap',[['toPmEnter',['$event']]]]]}}" bindtap="__e"></image><image class="list_image" mode="widthFix" src="{{auctionList[0].merchantAuctionLogo?auctionList[0].merchantAuctionLogo:'https://xssj.letterbook.cn/applet/images/bg_auctionList.png'}}"></image><block wx:for="{{auctionList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="list_item" data-auctionid="{{item.auctionId}}" data-event-opts="{{[['tap',[['toDetails',['$event']]]]]}}" bindtap="__e"><block wx:if="{{item.status==3}}"><image class="list_item_icon" src="https://xssj.letterbook.cn/applet/images/icon_over.png"></image></block><view class="list_item_image"><image src="{{item.logoImg}}" mode="aspectFill"></image><block wx:if="{{item.status==3}}"><view>已结束</view></block></view><view class="list_item_info"><view class="list_item_up"><view class="list_item_up_left"><view class="list_item_name">{{item.xsAuctionItem.auctionItemName}}</view><view class="list_item_desc">{{item.xsAuctionItem.description}}</view></view><view class="list_item_up_right"><view class="list_item_label">{{item.timeUnit==1?'即时喊':item.timeUnit==2?'短时喊':'长时喊'}}</view><view class="list_item_startPrice">{{"保底价：￥"+item.xsAuctionItem.startingPrice}}</view><block wx:if="{{item.auctionPrice!=0}}"><block><view class="list_item_nowPrice" style="{{(item.status=='3'?'color: #999999;':'')}}">{{''+(item.status=='3'?'成交价：￥':'当前价：￥')+''}}<text style="font-size:24rpx;">{{item.auctionPrice}}</text></view></block></block></view></view><view class="list_item_down"><block wx:if="{{item.status!='3'}}"><block><van-count-down vue-id="{{'55ad5e56-1-'+index}}" use-slot="{{true}}" data-index="{{index}}" time="{{item.timeMinutes}}" data-event-opts="{{[['^change',[['changeTime',['$event',['o',['index',index]]]]]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="list_item_onLookers"><block wx:if="{{item.status=='1'}}"><text>开始时间</text></block><block wx:if="{{item.status=='2'}}"><text style="color:#ff0000;">距离结束</text></block><block wx:if="{{item.timeData.days!=0}}"><text>{{item.timeData.days}}</text></block><block wx:if="{{item.timeData.days!=0}}"><text>天</text></block><text>{{item.timeData.hours<10?'0'+item.timeData.hours:item.timeData.hours}}</text><text>:</text><text>{{item.timeData.minutes<10?'0'+item.timeData.minutes:item.timeData.minutes}}</text><text>:</text><text>{{item.timeData.seconds<10?'0'+item.timeData.seconds:item.timeData.seconds}}</text></view></van-count-down><block wx:if="{{item.status=='1'}}"><view class="list_item_onLookers" style="color:#07c160;">{{"预约人数："+item.reserveNumber}}</view></block></block></block><block wx:if="{{item.status=='3'}}"><view style="color:#333333;">{{"结束时间 "+item.endTime}}</view></block><block wx:if="{{item.status!='1'}}"><view class="list_item_onLookers">{{item.personNumber+"人参与喊价"}}</view></block></view></view></view></block></block><block wx:if="{{auctionList.length==0&&reqComplete}}"><view class="me-fx-col me-fx-c-c empty-list"><text>抱歉！暂无数据</text></view></block></view>
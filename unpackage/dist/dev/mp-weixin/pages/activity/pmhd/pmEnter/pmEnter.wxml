<view class="pmEnter"><view class="order"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="order_item" data-status="{{index}}" data-event-opts="{{[['tap',[['toOrderList',['$event']]]]]}}" bindtap="__e"><view class="order_icon"><image src="{{'/image/pmhd/icon_pmorder'+(index+2)+'.png'}}"></image><block wx:if="{{item.num}}"><view class="order_num">{{item.num>99?'...':item.num}}</view></block></view><view class="order_name">{{item.name}}</view></view></block></block></view><block wx:for="{{statusList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="mypm" data-index="{{index}}" data-event-opts="{{[['tap',[['toPmList',['$event']]]]]}}" bindtap="__e"><view class="mypm_left">{{item}}</view><view class="mypm_right"><block wx:for="{{numList}}" wx:for-item="itemNum" wx:for-index="itemIndex" wx:key="itemIndex"><block wx:index="{{index}}"><block wx:if="{{itemNum.status==index+1}}"><view>{{itemNum.count}}</view></block></block></block><image src="/static/image/pmhd/icon_enter.png"></image></view></view></block></view>

.btn-container {
    z-index: 2;
    position: fixed;
    bottom: 0;
    right: 0;
}
.btn-main {
    width: 100rpx;
    height: 100rpx;
    position: absolute;
    bottom: 150rpx;
    right: 20rpx;
}
.btn-img {
    width: 100%;
    height: 100%;
}
.btn-num {
    width: 26rpx;
    height: 26rpx;
    line-height: 26rpx;
    background: #ff1d2e;
    border: 1px solid #ff1c30;
    border-radius: 50%;

    font-size: 22rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: #ffffff;
    opacity: 1;
    position: absolute;
    top: 0rpx;
    right: 0rpx;
    text-align: center;
}
.menuOpen {
    -webkit-animation: menuO 0.3s both;
            animation: menuO 0.3s both;
}
.menuClose {
    -webkit-animation: menuC 0.3s both;
            animation: menuC 0.3s both;
}
@-webkit-keyframes menuO {
0% {
        -webkit-transform: rotate(0deg);
                transform: rotate(0deg);
}
20% {
        -webkit-transform: rotate(90deg);
                transform: rotate(90deg);
}
40% {
        -webkit-transform: rotate(45deg);
                transform: rotate(45deg);
}
60% {
        -webkit-transform: rotate(135deg);
                transform: rotate(135deg);
}
80% {
        -webkit-transform: rotate(90deg);
                transform: rotate(90deg);
}
100% {
        -webkit-transform: rotate(0deg);
                transform: rotate(0deg);
}
}
@keyframes menuO {
0% {
        -webkit-transform: rotate(0deg);
                transform: rotate(0deg);
}
20% {
        -webkit-transform: rotate(90deg);
                transform: rotate(90deg);
}
40% {
        -webkit-transform: rotate(45deg);
                transform: rotate(45deg);
}
60% {
        -webkit-transform: rotate(135deg);
                transform: rotate(135deg);
}
80% {
        -webkit-transform: rotate(90deg);
                transform: rotate(90deg);
}
100% {
        -webkit-transform: rotate(0deg);
                transform: rotate(0deg);
}
}
@-webkit-keyframes menuC {
0% {
        -webkit-transform: rotate(0deg);
                transform: rotate(0deg);
}
20% {
        -webkit-transform: rotate(90deg);
                transform: rotate(90deg);
}
40% {
        -webkit-transform: rotate(45deg);
                transform: rotate(45deg);
}
60% {
        -webkit-transform: rotate(135deg);
                transform: rotate(135deg);
}
80% {
        -webkit-transform: rotate(90deg);
                transform: rotate(90deg);
}
100% {
        -webkit-transform: rotate(0deg);
                transform: rotate(0deg);
}
}
@keyframes menuC {
0% {
        -webkit-transform: rotate(0deg);
                transform: rotate(0deg);
}

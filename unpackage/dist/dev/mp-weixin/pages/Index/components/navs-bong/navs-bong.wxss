        -webkit-transform: rotate(135deg);
                transform: rotate(135deg);
}
80% {
        -webkit-transform: rotate(90deg);
                transform: rotate(90deg);
}
100% {
        -webkit-transform: rotate(0deg);
                transform: rotate(0deg);
}
}
.menu-item {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;

    border-radius: 50%;
    width: 100rpx;
    height: 100rpx;
    position: absolute;
}
.menu-icon {
    width: 88rpx;
    height: 8rpx;
    border-radius: 50%;
}
.menu-name {
    color: #607d8b;
    font-size: 25rpx;
    font-weight: 600;
}
.menu-in-animation1 {
    -webkit-animation: bounceInDown1 0.45s linear both;
            animation: bounceInDown1 0.45s linear both;
}
.menu-out-animation1 {
    -webkit-animation: bounceOutDown1 0.45s linear both;
            animation: bounceOutDown1 0.45s linear both;
}
.menu-in-animation2 {

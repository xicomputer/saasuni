<view class="invoice-container"><block wx:if="{{!Invoice.length}}"><view class="no-invoice"><image class="no-invoice-img" src="/static/pages/img/no_notice.png"></image><view class="no-invoice-text">您还没有添加发票……</view></view></block><block wx:if="{{!Invoice.length}}"><no-data vue-id="0e2b0eea-1" text="您还没有发票哦！" url="https://xssj.letterbook.cn/applet/images/sj_no_data_img2.png" bind:__l="__l"></no-data></block><block wx:else><view class="InvoiceList"><van-radio-group vue-id="0e2b0eea-2" value="{{radio}}" data-event-opts="{{[['^change',[['change']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{Invoice}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['backOrder',['$event']]]]]}}" style="position:relative;" bindtap="__e"><van-swipe-cell vue-id="{{('0e2b0eea-3-'+index)+','+('0e2b0eea-2')}}" right-width="{{' '+65+' '}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="InvoiceList-item" data-item="{{item}}" data-event-opts="{{[['tap',[['backOrder',['$event']]]]]}}" bindtap="__e"><van-radio vue-id="{{('0e2b0eea-4-'+index)+','+('0e2b0eea-3-'+index)}}" name="{{item.id}}" checked-color="#ec1919" icon-size="20px" data-com-type="wx" bind:__l="__l"></van-radio><view class="right"><view class="cpy-name">{{item.companyName}}</view><view class="cpy-number">{{"税号"+item.companyDp}}</view><view class="cpy-email">{{"邮箱"+item.email}}</view></view></view><view class="del" style="position:absolute;top:40rpx;right:30rpx;" data-id="{{item.id}}" data-event-opts="{{[['tap',[['clickDel',['$event']]]]]}}" bindtap="__e">删除</view></van-swipe-cell></view></block></van-radio-group></view></block><view class="addlist-footer-btn"><navigator class="saveBtn" url="../NewInvoice/NewInvoice">添加发票</navigator></view></view>